<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/roomList"
        android:layout_width="114dp"
        android:layout_height="169dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="259dp"
        android:layout_marginBottom="31dp" />

    <ListView
        android:id="@+id/userList"
        android:layout_width="114dp"
        android:layout_height="169dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="34dp"
        android:layout_marginBottom="28dp" />

    <include
        android:id="@+id/water_sensor_1"
        layout="@layout/content_water_sensor_button"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="112dp" />

    <include
        android:id="@+id/water_sensor_2"
        layout="@layout/content_water_sensor_button"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="224dp"
        android:layout_marginLeft="224dp"
        android:layout_marginTop="112dp" />

    <TextView
        android:id="@+id/water_sensor_title"
        android:layout_width="271dp"
        android:layout_height="34dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="67dp"
        android:gravity="center" />

    <TextView
        android:id="@+id/air_quality_sensor_title"
        android:layout_width="259dp"
        android:layout_height="51dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="294dp"
        android:gravity="center" />

    <include
        android:id="@+id/air_quality_1"
        layout="@layout/content_air_quality_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="361dp" />

    <ListView
        android:id="@+id/propertyList"
        android:layout_width="114dp"
        android:layout_height="169dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="144dp"
        android:layout_marginBottom="30dp" />

</RelativeLayout>